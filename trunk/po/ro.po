#: ../gnome-panel/panel-profile.c:1617 ../gnome-panel/panel-profile.c:1680
#, c-format
msgid "Error reading GConf integer value '%s': %s"
msgstr "Eroare la citirea valorii întregi GConf „%s”: %s"

#: ../gnome-panel/panel-profile.c:1554
#, c-format
msgid ""
"Panel '%s' is set to be displayed on screen %d which is not currently "
"available. Not loading this panel."
msgstr ""
"Panoul „%s” este setat pentru a fi afişat pe ecranul %d care nu este "
"disponibil momentan. Acest panou nu va porni."

#: ../gnome-panel/panel-profile.c:1632
#, c-format
msgid "Error reading GConf boolean value '%s': %s"
msgstr "Eroare la citirea valorii boolene GConf „%s”: %s"

#: ../gnome-panel/panel-properties-dialog.c:120
#: ../libpanel-applet/panel-test-applets.c:53
