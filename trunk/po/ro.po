#: ../applets/clock/clock.schemas.in.h:29
msgid "Use UNIX time"
msgstr "Utilizează timpul Unix"

#: ../applets/clock/clock.schemas.in.h:30
msgid "Use UTC"
msgstr "Utilizează UTC"

