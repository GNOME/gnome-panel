msgstr "Nhận được kiểu nền không hoàn chỉnh '%s'"

#:../libpanel-applet/panel-applet.c:1009
#,c-format
msgid "Incomplete '%s' background type received: %s"
msgstr "Nhận được kiểu nền không hoàn chỉnh '%s': %s"

#:../libpanel-applet/panel-applet.c:1018
#,c-format
msgid "Failed to get pixmap %s"
msgstr "Lỗi khi lấy ảnh bằng điểm %s"

#:../libpanel-applet/panel-applet.c:1025
msgid "Unknown background type received"
msgstr "Nhận được một kiểu nền lạ"

#:../libpanel-applet/panel-applet.c:1210
msgid "The Applet's containing Panel's orientation"
msgstr "Tiểu dụng chứa hướng Bảng điều khiển"

#:../libpanel-applet/panel-applet.c:1218
msgid "The Applet's containing Panel's size in pixels"
msgstr "Tiểu dụng chứa kích thước Panel (theo điểm ảnh)"

#:../libpanel-applet/panel-applet.c:1226
msgid "The Applet's containing Panel's background color or pixmap"
msgstr "Tiểu dụng chứa màu nền hoặc ảnh bằng điểm nền của Bảng điều khiển"

#:../libpanel-applet/panel-applet.c:1234
msgid "The Applet's flags"
msgstr "Cờ hiệu của tiểu dụng"

#:../libpanel-applet/panel-applet.c:1242
msgid "Ranges that hint what sizes are acceptable for the applet"
msgstr "Phạm vi mà gợi ý kích thước nào chấp nhận được đối với tiểu dụng"

#:../libpanel-applet/panel-applet.c:1250
msgid "The Applet's containing Panel is locked down"
msgstr "Bảng điều khiển chứa tiểu dụng ấy bị khóa."

#:../libpanel-applet/panel-test-applets.c:32
msgid "Specify an applet IID to load"
msgstr "Xác định IID của tiểu dụng cần tải"

