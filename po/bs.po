#: applets/clock/clock.schemas.in.h:40
msgid "Use UNIX time"
msgstr "Koristi UNIX vrijeme"

#: applets/clock/clock.schemas.in.h:41
msgid "Use UTC"
msgstr "Koristi UTC"

