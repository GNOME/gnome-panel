#: ../applets/clock/clock.schemas.in.h:29
msgid "Use UNIX time"
msgstr "Utilisar l'ora UNIX"

#: ../applets/clock/clock.schemas.in.h:30
msgid "Use UTC"
msgstr "Utilisar l'ora UTC"

