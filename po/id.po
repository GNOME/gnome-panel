#: ../applets/clock/clock.schemas.in.h:29
msgid "Use UNIX time"
msgstr "Gunakan waktu UNIX"

#: ../applets/clock/clock.schemas.in.h:30
msgid "Use UTC"
msgstr "Gunakan UTC"

