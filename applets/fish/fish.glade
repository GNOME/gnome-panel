		  <property name="padding">0</property>
		  <property name="expand">False</property>
		  <property name="fill">False</property>
		</packing>
	      </child>

	      <child>
		<widget class="GnomePixmapEntry" id="image_entry">
		  <property name="visible">True</property>
		  <property name="history_id">fish_animation</property>
		  <property name="max_saved">10</property>
		  <property name="browse_dialog_title" translatable="yes">Browse</property>
		  <property name="modal">False</property>
		  <property name="do_preview">True</property>
		  <accessibility>
		    <atkrelation target="label8" type="labelled-by"/>
		  </accessibility>

		  <child internal-child="entry">
		    <widget class="GtkEntry" id="image_entry1">
		      <property name="visible">True</property>
		      <property name="can_focus">True</property>
		      <property name="editable">True</property>
