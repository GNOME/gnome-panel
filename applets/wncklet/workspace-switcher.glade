                        <child>
                          <widget class="GtkTreeView" id="workspace_tree_view">
                            <property name="can_focus">yes</property>
                            <property name="headers-visible">no</property>
                            <property name="rules_hint">yes</property>
                            <property name="visible">yes</property>
                          </widget>
