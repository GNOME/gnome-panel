<?xml version="1.0"?>
<gconfschemafile>

  <schemalist>    

    <!-- Applies to all object types -->

    <schema>
      <key>/schemas/apps/panel/objects/object_type</key>
      <owner>panel</owner>
      <type>string</type>
      <default>menu-object</default>
      <locale name="C">
        <short>The type of panel object</short>
        <long>
          The type of this panel object. There are several possible
          values - "drawer-object", "menu-object", "launcher-object",
          "bonobo-applet", "action-applet" and "menu-bar".
        </long>
      </locale>
    </schema>
   
    <schema>
      <key>/schemas/apps/panel/objects/toplevel_id</key>
      <owner>panel</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>The panel toplevel which contains this object</short>
        <long>
          The identifier of the panel toplevel which contains this object.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/panel/objects/position</key>
      <owner>panel</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>The object's position on the panel</short>
        <long>
          The position of this panel object. The position is specified
          by the number of pixels from the left (or top if vertical)
          panel edge.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/panel/objects/panel_right_stick</key>
      <owner>panel</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Interpret the position relative to the bottom/right edge</short>
        <long>
          If true the position of the object is interpreted relative
          to the right (or bottom if vertical) edge of the panel.
        </long>
      </locale>
    </schema>

    <!-- Applies to applets -->

    <schema>
      <key>/schemas/apps/panel/objects/bonobo_iid</key>
      <owner>panel</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>The Bonobo IID of the applet</short>
        <long>
          The Bonobo implementation ID of the applet - e.g.
          "OAFIID:GNOME_ClockApplet". This setting is only relevant if
          the object_type is "bonobo-applet".
        </long>
      </locale>
    </schema>

    <!-- Applies to drawers -->

    <schema>
      <key>/schemas/apps/panel/objects/attached_toplevel_id</key>
      <owner>panel</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>The panel attached to this drawer</short>
        <long>
          The identifier of the panel attached to this drawer. This
          setting is only relevant if the object_type is
          "drawer-object".
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/panel/objects/tooltip</key>
      <owner>panel</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>The tooltip displayed for this drawer</short>
        <long>
          The text to display in a tooltip for this drawer. This
          setting is only relevant if the object_type is
          "drawer-object".
        </long>
      </locale>
    </schema>

    <!-- Applies to drawers and menu buttons -->

    <schema>
      <key>/schemas/apps/panel/objects/use_custom_icon</key>
      <owner>panel</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Use a custom icon for the object's button</short>
        <long>
          If true, the custom_icon setting is used as a custom icon for
          the button. If false, the custom_icon setting is ignored.
          This setting is only relevant if the object_type is
          "menu-object" or "drawer-object".
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/panel/objects/custom_icon</key>
      <owner>panel</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>The icon used for the object's button</short>
        <long>
          The location of the image file used as the icon for the
          object's button. This setting is only relevant if the
          object_type is "drawer-object" or "menu-object" and
          use_custom_icon is true.
        </long>
      </locale>
    </schema>

    <!-- Applies to menu buttons -->

    <schema>
      <key>/schemas/apps/panel/objects/use_menu_path</key>
      <owner>panel</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Use a custom path for the menu contents</short>
        <long>
          If true, the menu_path setting is used as the path from which
          the menu contents should be constructed. If false, the
          menu_path setting is ignored. This setting is only relevant
          if the object_type is "menu-object".
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/panel/objects/menu_path</key>
      <owner>panel</owner>
      <type>string</type>
      <default>applications:///</default>
      <locale name="C">
        <short>The path for the menu contents</short>
        <long>
          The path from which the menu contents is contructed. The
          setting is only relevant if use_menu_path is true and the
          object_type is "menu-object".
        </long>
      </locale>
    </schema>

    <!-- Appies to launchers -->

    <schema>
      <key>/schemas/apps/panel/objects/launcher_location</key>
      <owner>panel</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>The location of the launcher</short>
        <long>
          The location of the .desktop file describing the launcher.
          This setting is only relevant if the object_type is
          "launcher-object".
        </long>
      </locale>
    </schema>

    <!-- Appies to action buttons -->

    <schema>
      <key>/schemas/apps/panel/objects/action_type</key>
      <owner>panel</owner>
      <type>string</type>
      <default>lock</default>
      <locale name="C">
        <short>The type of action button</short>
        <long>
          The action type this button represents. Possible values are
          "lock", "logout", "run", "search" and "screenshot". This
          setting is only relevant if the object_type is
	  "action-applet".
        </long>
      </locale>
    </schema>
         
  </schemalist>
  
</gconfschemafile>
