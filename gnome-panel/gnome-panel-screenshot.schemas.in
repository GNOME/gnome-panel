<?xml version="1.0"?>

<gconfschemafile>
  <schemalist>    

    <schema>
      <key>/schemas/apps/gnome_panel_screenshot/web_dir</key>
      <applyto>/apps/gnome_panel_screenshot/web_dir</applyto>
      <owner>gnome_panel_screenshot</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Web directory</short>
        <long>The user's directory in which screenshots should be
              saved so as to appear on the web.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome_panel_screenshot/drop_shadow_window</key>
      <applyto>/apps/gnome_panel_screenshot/drop_shadow_window</applyto>
      <owner>gnome_panel_screenshot</owner>
      <type>boolean</type>
      <default>false</default>
      <locale name="C">
        <short>Drop Shadow Window</short>
        <long>Add a drop shadow to screen shots of individual windows</long>
      </locale>
    </schema>

  </schemalist>
</gconfschemafile>
