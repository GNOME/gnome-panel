<?xml version="1.0"?>

<gconfschemafile>
  <schemalist>    

    <schema>
      <key>/schemas/apps/gnome_panel_screenshot/last_save_directory</key>
      <applyto>/apps/gnome_panel_screenshot/last_save_directory</applyto>
      <owner>gnome_panel_screenshot</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Web directory</short>
        <long>Directory that the screenshooter last used to save in</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome_panel_screenshot/include_border</key>
      <applyto>/apps/gnome_panel_screenshot/include_border</applyto>
      <owner>gnome_panel_screenshot</owner>
      <type>boolean</type>
      <default>true</default>
      <locale name="C">
        <short>Include Border</short>
        <long>Include the window manager border along with the screenshot</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome_panel_screenshot/border_effect</key>
      <applyto>/apps/gnome_panel_screenshot/border_effect</applyto>
      <owner>gnome_panel_screenshot</owner>
      <type>string</type>
      <default>none</default>
      <locale name="C">
        <short>Border Effect</short>
        <long>Effect to add to the outside of a border.  Possible values
      are "shadow", "none", and "black-line".
      </long>
      </locale>
    </schema>

  </schemalist>
</gconfschemafile>
