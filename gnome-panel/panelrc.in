# gtkrc-style file for all panel applets.  Add any needed features here.

# style <name> [= <name>]
# {
#   <option>
# }
#
# widget <widget_set> style <style_name>
# widget_class <widget_class_set> style <style_name>
# accelerator <widget_name> <accelerator>

pixmap_path "@datadir@/panel/"

style "netwatch-modem-up"
{
  bg_pixmap[NORMAL] = "netwatch-modem-up.xpm"
  bg_pixmap[ACTIVE] = "netwatch-modem-up.xpm"
  bg_pixmap[PRELIGHT] = "netwatch-modem-up.xpm"
  bg_pixmap[SELECTED] = "netwatch-modem-up.xpm"
  bg_pixmap[INSENSITIVE] = "netwatch-modem-up.xpm"
}

style "netwatch-modem-down"
{
  bg_pixmap[NORMAL] = "netwatch-modem-down.xpm"
  bg_pixmap[ACTIVE] = "netwatch-modem-down.xpm"
  bg_pixmap[PRELIGHT] = "netwatch-modem-down.xpm"
  bg_pixmap[SELECTED] = "netwatch-modem-down.xpm"
  bg_pixmap[INSENSITIVE] = "netwatch-modem-down.xpm"
}

style "netwatch-net-up"
{
  bg_pixmap[NORMAL] = "netwatch-net-up.xpm"
  bg_pixmap[ACTIVE] = "netwatch-net-up.xpm"
  bg_pixmap[PRELIGHT] = "netwatch-net-up.xpm"
  bg_pixmap[SELECTED] = "netwatch-net-up.xpm"
  bg_pixmap[INSENSITIVE] = "netwatch-net-up.xpm"
}

style "netwatch-net-down"
{
  bg_pixmap[NORMAL] = "netwatch-net-down.xpm"
  bg_pixmap[ACTIVE] = "netwatch-net-down.xpm"
  bg_pixmap[PRELIGHT] = "netwatch-net-down.xpm"
  bg_pixmap[SELECTED] = "netwatch-net-down.xpm"
  bg_pixmap[INSENSITIVE] = "netwatch-net-down.xpm"
}


widget "*.netwatch_ppp_up" style "netwatch-modem-up"
widget "*.netwatch_ppp_down" style "netwatch-modem-down"
widget "*.netwatch_slip_up" style "netwatch-modem-up"
widget "*.netwatch_slip_down" style "netwatch-modem-down"
widget "*.netwatch_plip_up" style "netwatch-modem-up"
widget "*.netwatch_plip_down" style "netwatch-modem-down"
widget "*.netwatch_eth_up" style "netwatch-net-up"
widget "*.netwatch_eth_down" style "netwatch-net-down"
widget "*.netwatch_tr_up" style "netwatch-net-up"
widget "*.netwatch_tr_down" style "netwatch-net-down"
widget "*.netwatch_arc_up" style "netwatch-net-up"
widget "*.netwatch_arc_down" style "netwatch-net-down"
widget "*.netwatch_atp_up" style "netwatch-net-up"
widget "*.netwatch_atp_down" style "netwatch-net-down"
