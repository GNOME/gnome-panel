          </value>
          <value>
            <string>clock</string>
          </value>
          <value>
            <string>show_desktop_button</string>
          </value>
          <value>
            <string>window_list</string>
          </value>
          <value>
            <string>workspace_switcher</string>
          </value>
        </list>
      </value>
    </entry>

  </entrylist>

  <!-- Top Panel -->

  <entrylist base="/apps/panel/default_setup/toplevels/top_panel">
    <entry>
      <key>name</key>
      <schema_key>/schemas/apps/panel/toplevels/name</schema_key>
